<!DOCTYPE dwr PUBLIC
    "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN"
    "http://getahead.org/dwr/dwr20.dtd">

<dwr>
	<allow>
		<create creator="spring" javascript="gadgetRenderService">
			<param name="beanName" value="gadgetRenderService" />
		</create>
		<convert match="org.jerrymouse.web.bean.Gadget" converter="bean">
			<param name="include"
				value="name,html,javascripts,javascriptSrc,title,,relativeUrl,htmlId"></param>
		</convert>
		<convert match="org.jerrymouse.web.bean.GagetContainer"
			converter="bean">
			<param name="include" value="leftGadget,middleGadget,rightGadget,title"></param>
		</convert>
		<convert match="org.jerrymouse.web.bean.MainContainer"
			converter="bean">
			<param name="include" value="tabs"></param>
		</convert>
	</allow>
</dwr>
